{
  "globals": {
    "jQuery": true,
    "$": true
  },
  "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "quotes": [1, "double"]
    "semi" : 2,
    "curly": [1, "double"]
  }
}